From a4af9ef1816f381469c0f128a86e08410af46fed Mon Sep 17 00:00:00 2001
From: Sebastien Bacher <seb128@ubuntu.com>
Date: Tue, 8 Sep 2020 16:24:39 +0200
Subject: [PATCH] Build using libm in some of the backends

---
 backend/dvi/meson.build  | 1 +
 backend/tiff/meson.build | 2 +-
 2 files changed, 2 insertions(+), 1 deletion(-)

diff --git a/backend/dvi/meson.build b/backend/dvi/meson.build
index c3bea84d..26aba04f 100644
--- a/backend/dvi/meson.build
+++ b/backend/dvi/meson.build
@@ -12,6 +12,7 @@ deps = backends_deps + [
   libspectre_dep,
   kpathsea_dep,
   t1_dep,
+  m_dep
 ]
 
 shared_module(
diff --git a/backend/tiff/meson.build b/backend/tiff/meson.build
index bb14a1d2..91b2b052 100644
--- a/backend/tiff/meson.build
+++ b/backend/tiff/meson.build
@@ -7,7 +7,7 @@ shared_module(
   backend_name,
   sources: sources,
   include_directories: backends_incs,
-  dependencies: backends_deps + [libtiff_dep],
+  dependencies: backends_deps + [libtiff_dep, m_dep],
   c_args: backends_cflags,
   link_args: backends_ldflags,
   link_depends: backends_symbol_map,
-- 
2.25.1

