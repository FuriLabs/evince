Package: evince-gtk
Section: x11
Conflicts: evince
Architecture: any
Depends: ${shlibs:Depends},
         ${misc:Depends},
         libevince1 (= ${binary:Version}),
         evince-common (>= ${gnome:Version}),
         evince-common (<< ${gnome:NextVersion}),
